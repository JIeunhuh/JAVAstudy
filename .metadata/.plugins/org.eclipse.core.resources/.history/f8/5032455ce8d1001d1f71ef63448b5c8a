package Chapter2;

public class Test3 {
	public static void main(String[] args) {
		
		byte age; // byte = 1byte , 첫번째 byte = 1이면 음수, 0이면 양수
		short point; // short = 2byte
		int price; // int = 4byte
		long totalSales; // long = 8byte
		
		age = 25;
		point = 32000;
		price = 50000;
		totalSales = e685147387l;
		
	}
}
