package Chapter2;

public class Test13 {
	public static void main(String[] args) {
		
		byte a = 0b1001; // 2진수 표현은 0b로 시작
		byte b = 0b1011;
	//	byte c = a & b; 연산결과를 
		int c = a & b; // bit연산 &   
		System.out.println("c = "+c);
		System.out.println("c = "+Integer.toBinaryString(c));
		System.out.println(Byte.toString(b));
		
		int e = a ^ b;
		System.out.println(e);
	}
} 
